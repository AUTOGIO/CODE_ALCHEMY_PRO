# CODE_ALCHEMY Professional - Production Environment Example
# Copy this to .env and configure with your actual credentials

# System Configuration
ENVIRONMENT=production
DEBUG_MODE=false
LOG_LEVEL=INFO

# LM Studio Configuration
LM_STUDIO_URL=http://localhost:1234
LM_STUDIO_TIMEOUT=60

# Apple Silicon Optimization
APPLE_SILICON_OPTIMIZATION=true
NEURAL_ENGINE_ENABLED=true
UNIFIED_MEMORY_LIMIT=16GB
PARALLEL_PROCESSING=true

# Data Directories
DATA_DIR=data
DOCUMENTS_DIR=data/documents
REPORTS_DIR=data/reports
CACHE_DIR=data/cache
LOGS_DIR=data/logs

# GitHub Integration (Add your actual token)
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username
GITHUB_AUTO_COMMIT=true

# Google Drive Integration (Add your actual credentials)
GOOGLE_DRIVE_ENABLED=false
GOOGLE_DRIVE_CREDENTIALS_FILE=credentials.json
GOOGLE_DRIVE_SYNC_FOLDERS=~/Desktop/CODE_ALCHEMY_Projects,~/Documents/AI_Projects

# MCP Server Configuration
MCP_ENABLED=true
MCP_SERVER_URL=http://localhost:1234

# Security Settings
SECURITY_MONITORING_ENABLED=true
CONTINUOUS_MONITORING=true

# Performance Settings
REAL_TIME_ANALYSIS=true
DYNAMIC_CACHING=true
CORE_UTILIZATION=8

# Model Configuration
DEFAULT_MODEL=deepseek-r1-0528-qwen3-8b
FALLBACK_MODEL=meta-llama-3.1-8b-instruct
REASONING_MODEL=microsoft_phi-4-mini-reasoning
FAST_MODEL=tinyllama-1.1b-chat-v1.0-intel-dpo 